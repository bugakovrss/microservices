{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Pivotal": "Debug",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "smarthome-controlapi-app"
    }
  },
  "consul": {
    "host": "consul-discovery",
    "port": 8500,
    "discovery": {
      "enabled": true,
      "register": true,
      "port": "8080",
      "ipAddress": "smarthome-controlapi",
      "preferIpAddress": true
    }
  }

}
