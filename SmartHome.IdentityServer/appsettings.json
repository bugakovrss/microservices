{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "smarthome-identity-app"
    }
  },
  "consul": {
    "host": "consul-discovery",
    "port": 8500,
    "discovery": {
      "enabled": true,
      "register": true,
      "port": "8095",
      "ipAddress": "smarthome-identity",
      "preferIpAddress": true
    }
  }
}
